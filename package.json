{
    "version": "1.0.0-beta.5",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "description": "Tiny state management library for react.",
    "repository": {
        "url": "https://github.com/ArtiomTr/stocked"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "npm run test -- --silent --ci --coverage --json --outputFile=report.json",
        "test:lcov": "npm run test -- --silent --ci --coverage",
        "lint": "tsdx lint",
        "lint:config": "npm run lint -- --write-file",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "release": "np"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "prettier": {
        "printWidth": 120,
        "semi": true,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "stocked",
    "author": "Artiom Tretjakovas",
    "module": "dist/stocked.esm.js",
    "size-limit": [
        {
            "path": "dist/stocked.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/stocked.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^4.6.2",
        "@testing-library/react-hooks": "^3.4.2",
        "@types/lodash": "^4.14.162",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "np": "^6.5.0",
        "prettier": "^1.19.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "size-limit": "^4.6.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.2.0",
        "typescript": "^4.1.2"
    },
    "dependencies": {
        "lodash": "^4.17.20",
        "lodash-es": "^4.17.15",
        "tiny-invariant": "^1.1.0"
    }
}
